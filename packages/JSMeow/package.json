{
	"name": "jsmeow",
	"version": "1.0.0",
	"types": "dist/index.d.ts",
	"main": "./dist/index.js",
	"author": "DroidXrX <droidxrx@gmail.com> (https://github.com/droidxrx)",
	"homepage": "https://github.com/droidxrx/DroidXrX/tree/master/packages/JSMeow#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/droidxrx/droidxrx-node.git",
		"directory": "packages/JSMeow"
	},
	"license": "MIT",
	"keywords": [
		"JSMeow",
		"pyMeow",
		"hacking",
		"esp",
		"read-memory",
		"write-memory"
	],
	"private": false,
	"engines": {
		"node": ">=16.14.0"
	},
	"os": [
		"win32"
	],
	"cpu": [
		"x64"
	],
	"scripts": {
		"install": "ECHO ''",
		"buildAddon": "cls && rimraf dist/*.node && node-gyp rebuild --release",
		"build-ts-production": "SET NODE_ENV=production && cls && rimraf dist/*.js temp && webpack",
		"build-ts-development": "SET NODE_ENV=development && cls && rimraf dist/*.js temp && webpack",
		"build-dts": "cls && dts-bundle-generator -o dist/index.d.ts --project ./tsconfig.json src/ts/index.ts --no-banner --no-check"
	}
}

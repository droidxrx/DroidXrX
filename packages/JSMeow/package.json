{
	"name": "@droidxrx/jsmeow",
	"version": "1.0.0",
	"types": "dist/index.d.ts",
	"main": "./dist/cjs/index.js",
	"exports": {
		".": {
			"require": "./dist/cjs/index.js",
			"default": "./dist/esm/index.js"
		}
	},
	"module": "./dist/esm/index.js",
	"author": "DroidXrX <droidxrx@gmail.com> (https://github.com/droidxrx)",
	"homepage": "https://github.com/droidxrx/DroidXrX/tree/master/packages/JSMeow#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/droidxrx/droidxrx-node.git",
		"directory": "packages/JSMeow"
	},
	"license": "MIT",
	"keywords": [
		"JSMeow",
		"pyMeow",
		"hacking",
		"esp",
		"read-memory",
		"write-memory"
	],
	"private": false,
	"engines": {
		"node": ">=16.14.0"
	},
	"os": [
		"win32"
	],
	"cpu": [
		"x64"
	],
	"scripts": {
		"build-addon": "cmake-js rebuild",
		"build-cjs": "npx tsc -p ./tsconfig.cjs.json",
		"build-esm": "npx tsc -p ./tsconfig.esm.json",
		"build": "npx rimraf dist build && npm run build-cjs && npm run build-esm && npm run build-addon"
	},
	"dependencies": {
		"node-addon-api": "^4.3.0"
	},
	"cmake-js": {
		"runtime": "node",
		"arch": "x64"
	},
	"clean-publish": {
		"withoutPublish": true,
		"tempDir": "package",
		"files": [
			"build",
			"src",
			"test",
			"tsconfig.cjs.json",
			"tsconfig.esm.json"
		]
	}
}

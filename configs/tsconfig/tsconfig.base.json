{
	"compilerOptions": {
		"module": "ES2022",
		"target": "ES2021",
		"esModuleInterop": true,
		"moduleResolution": "node",
		"allowSyntheticDefaultImports": true,
		"alwaysStrict": false,
		"noImplicitUseStrict": false,
		"keyofStringsOnly": false,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"strictNullChecks": true,
		"noImplicitAny": true,
		"allowJs": false,
		"removeComments": true,
		"pretty": false
	}
}
